<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.cents635366" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cents</name>
    <description>
    Cents
  </description>
    <author email="support@cents.com" href="http://cents.com/">
    Cents
  </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="ShowSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="android-build-tool" value="gradle" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.InAppBrowser" />
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <gap:plugin name="cordova-plugin-admob" source="npm" />
    <gap:plugin name="cordova-plugin-inappbrowser" source="npm" spec="1.5.0" />
    <gap:plugin name="ionic-plugin-keyboard" source="npm" version="2.2.0" />
    <gap:plugin name="cordova-plugin-camera" source="npm" version="2.3.0" />
    <gap:plugin name="cordova-splashscreen" source="npm" spec="3.1.0-3" />
    <gap:plugin name="cordova-plugin-statusbar" source="npm" spec="2.2.0" />
    <gap:plugin name="cordova-plugin-device" source="npm" spec="1.1.3" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" version="1.3.0" />
    <gap:plugin name="test-private-alert-plugin" source="npm" version="1.0.0" />
    <platform name="ios">
        <icon height="60" src="img/icons/ios/Icon-60.png" width="60" />
        <icon height="120" src="img/icons/ios/Icon-60@2x.png" width="120" />
        <icon height="180" src="img/icons/ios/Icon-60@3x.png" width="180" />
        <icon height="76" src="img/icons/ios/Icon-76.png" width="76" />
        <icon height="152" src="img/icons/ios/Icon-76@2x.png" width="152" />
        <icon height="40" src="img/icons/ios/Icon-Small-40.png" width="40" />
        <icon height="80" src="img/icons/ios/icon-Small-40@2x.png" width="80" />
        <icon height="57" src="img/icons/ios/Icon.png" width="57" />
        <icon height="114" src="img/icons/ios/Icon@2x.png" width="114" />
        <icon height="72" src="img/icons/ios/Icon-72.png" width="72" />
        <icon height="144" src="img/icons/ios/Icon-72@2x.png" width="144" />
        <icon height="29" src="img/icons/ios/Icon-Small.png" width="29" />
        <icon height="58" src="img/icons/ios/Icon-Small@2x.png" width="58" />
        <icon height="50" src="img/icons/ios/Icon-Small-50.png" width="50" />
        <icon height="100" src="img/icons/ios/Icon-Small-50@2x.png" width="100" />
        <splash src="img/splashscreen/Splash1.png" />
        <splash gap:platform="ios" height="960" src="img/splashscreen/ios/Default@2x.png" width="640" />
        <splash gap:platform="ios" height="1136" src="img/splashscreen/ios/Default-568h@2x.png" width="640" />
        <splash gap:platform="ios" height="1334" src="img/splashscreen/ios/Default-667h@2x.png" width="750" />
        <splash gap:platform="ios" height="2208" src="img/splashscreen/ios/Default-Portrait-736h@3x.png" width="1242" />
        <splash gap:platform="ios" height="1242" src="img/splashscreen/ios/Default-Landscape-736h@3x.png" width="2208" />
        <splash gap:platform="ios" height="1024" src="img/splashscreen/ios/Default-Portrait.png" width="768" />
        <splash gap:platform="ios" height="768" src="img/splashscreen/ios/Default-Landscape.png" width="1024" />
        <splash gap:platform="ios" height="2048" src="img/splashscreen/ios/Default-Portrait@2x.png" width="1536" />
        <splash gap:platform="ios" height="1536" src="img/splashscreen/ios/Default-Landscape@2x.png" width="2048" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="img/icons/android/Icon-ldpi.png" />
        <icon density="mdpi" src="img/icons/android/Icon-mdpi.png" />
        <icon density="hdpi" src="img/icons/android/Icon-hdpi.png" />
        <icon density="xhdpi" src="img/icons/android/Icon-xhdpi.png" />
        <icon density="xxhdpi" src="img/icons/android/Icon-xxhdpi.png" />
        <icon density="xxxhdpi" src="img/icons/android/Icon-xxxhdpi.png" />
        <splash src="img/splashscreen/Splash1.png" />
        <splash gap:qualifier="ldpi" src="img/splashscreen/Splash1.png" />
        <splash gap:qualifier="mdpi" src="img/splashscreen/Splash1.png" />
        <splash gap:qualifier="hdpi" src="img/splashscreen/Splash1.png" />
        <splash gap:qualifier="xhdpi" src="img/splashscreen/Splash1.png" />
    </platform>
    <platform name="browser">
        <preference name="SplashScreen" value="img/splashscreen/Splash1.png" />
        <preference name="SplashScreenDelay" value="3000" />
        <preference name="SplashScreenBackgroundColor" value="green" />
        <preference name="ShowSplashScreen" value="true" />
        <preference name="SplashScreenWidth" value="600" />
        <preference name="SplashScreenHeight" value="300" />
    </platform>
    <gap:config-file overwrite="true" parent="LSApplicationQueriesSchemes" platform="ios">
        <array>
            <string>fbapi</string>
            <string>fbapi20130214</string>
            <string>fbapi20130410</string>
            <string>fbapi20130702</string>
            <string>fbapi20131010</string>
            <string>fbapi20131219</string>
            <string>fbapi20140410</string>
            <string>fbapi20140116</string>
            <string>fbapi20150313</string>
            <string>fbapi20150629</string>
            <string>fbauth</string>
            <string>fbauth2</string>
            <string>fb-messenger-api20140430</string>
        </array>
    </gap:config-file>
    <platform name="android">
        <preference name="android-minSdkVersion" value="15" />
    </platform>
</widget>
